    "graphql": "15.7.2",
    "he": "1.2.0",
    "jsdom": "18.0.1",
    "loader-utils": "3.1.3",
    "next": "12.0.3",
    "next-compose-plugins": "2.2.1",
    "nextjs-progressbar": "0.0.13",
    "parameterize": "1.0.0",
    "path": "0.12.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "4.3.1",
